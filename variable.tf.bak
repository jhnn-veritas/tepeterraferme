variable "vpc_cidr" {
  description = "Le bloc addresse de la VPC"
  type        = string
  default     = "10.0.0.0/16"
}
variable "subnet_cidr" {
  description = "Le sous-reso public"
  type        = string
  default     = "10.0.1.0/24"
}
variable "subnet2_cidr" {
  description = "Le bloc d'adresses IP pour le deuxième sous-réseau"
  type        = string
  default     = "10.0.2.0/24"
}